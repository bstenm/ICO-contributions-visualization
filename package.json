{
  "name": "ico-contributions-visualization",
  "author": "Bertrand Steinmetz",
  "version": "0.1.0",
  "description": "An app to visualize ico contributions data",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.1.0",
    "@svgr/webpack": "2.4.1",
    "apollo-boost": "0.1.23",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.3",
    "babel-preset-react-app": "^6.1.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "chart.js": "^1.1.1",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "graphql": "14.0.2",
    "graphql-request": "1.8.2",
    "graphql-tag": "2.10.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "lodash": "4.17.11",
    "mini-css-extract-plugin": "0.4.3",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pm2": "3.2.4",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "15.6.2",
    "react": "^16.6.3",
    "react-apollo": "2.3.3",
    "react-app-polyfill": "^0.1.3",
    "react-chartjs": "1.2.0",
    "react-dev-utils": "^6.1.1",
    "react-dom": "^16.6.3",
    "react-redux": "6.0.0",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "resolve": "1.8.1",
    "safe-graphql-yoga": "0.0.0-semantic-release",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "waait": "1.0.2",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.9",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "scripts": {
    "watch-client": "node client/scripts/start.js",
    "build-client": "node client/scripts/build.js",
    "test-client": "node client/scripts/test.js  --config jest.config.client.json",
    "lint": "eslint --debug client/src/ server/",
    "lint-write": "eslint --debug client/src/ server/ --fix",
    "prettier": "prettier --write client/src/**/*.js server/*.js",
    "watch-server": "NODE_ENV=development pm2-dev server/index.js",
    "test-server": "NODE_ENV=test jest --forceExit --config jest.config.server.json",
    "start": "node server/index.js",
    "watch-server-debug": "NODE_ENV=development nodemon --inspect server/index.js",
    "watch-app": "concurrently -k -p \"[{name}]\" -n \"React,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn watch-client\" \"yarn watch-server\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "yarn lint-write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "tabWidth": 6,
    "semi": true
  },
  "devDependencies": {
    "concurrently": "4.1.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-jest-enzyme": "7.0.1",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-jest": "22.0.0",
    "eslint-plugin-prettier": "3.0.0",
    "husky": "1.1.4",
    "lint-staged": "8.0.5",
    "prettier": "1.15.2"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
